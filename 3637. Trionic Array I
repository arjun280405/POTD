class Solution {
public:
    bool isTrionic(vector<int>& nums) {
        int n=nums.size();
        //first check for strictly increasing 
        int i=0;
        while(i+1<n && nums[i]<nums[i+1]){
            i++;
        }
        if(i==0 ||i==n-1)return false;//only increasing
        //decreasing
        while(i+1<n && nums[i]>nums[i+1])
        {
            i++;
        }
        if(i==n-1)return false;
        //Increasing
        while(i+1<n && nums[i]<nums[i+1])
        i++;
        return i==n-1;
    }
};
