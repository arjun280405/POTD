class Solution {
public:
  //No role of Collision in question only for diverting mind
    int getLastMoment(int n, vector<int>& left, vector<int>& right) {
        int result=0;
        for(auto &x:left){
            result=max(result,x);//for goint to left ants
        }
        for(auto &x:right){
            result=max(result,n-x);//for right 
        }
        return result;
    }
};
